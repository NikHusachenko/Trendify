    <div class="col-md-3">Замовлення:</div>
    <div class="col-md-3"></div>
    <div class="col-2">
        <a class="text-dark" asp-area="" asp-controller="Home" asp-action="Privacy">
            <span class="material-symbols-outlined">
                add_circle
            </span>
        </a>
    </div>
    <div class="col-2">
        <a class="text-dark" asp-area="" asp-controller="Home" asp-action="Privacy">
            <span class="material-symbols-outlined">
                cancel
            </span>
        </a>
    </div>
    <div class="col-2">
        <a class="text-dark" asp-area="" asp-controller="Home" asp-action="Privacy">
            <span class="material-symbols-outlined">
                change_circle
            </span>
        </a>
    </div>
    <div>
        <table class="table table-hover table-bordered table-secondary" style="margin: 0; padding: 0;">
            <tbody>
                <tr>
                    <th>Ід</th>
